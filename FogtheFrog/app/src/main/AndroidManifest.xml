<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fogthefrog">

    <application
        android:allowBackup="true"
        android:icon="@drawable/froggie"
        android:label="@string/app_name"
        android:roundIcon="@drawable/froggie"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionsScreen" />
        <activity android:name=".TitleScreen"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name=".HighScores" />
        <activity android:name=".GameOver" />
        <activity android:name=".Game" />
    </application>

</manifest>